@#!/bin/sh -


mydir=`dirname "$0"`
mydir=`(cd "$mydir" && pwd -L)`

tmpl="${mydir}/../etc/htemplate"

nam=`basename "$1"`
nam=`echo "$nam"|tr a-z A-Z|sed 's/\./_/g'`
nam="${nam}_"

cat "$tmpl" | sed 's/@@(hdr)/'"${nam}"'/g'



# EOF
@
